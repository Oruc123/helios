  "private": true,
  "scripts": {
    "build": "npm run lint && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "lint": "eslint ./src -c .eslintrc --ext js,jsx,ts,tsx",
    "check-staged": "lint-staged",
    "start": "npm run lint && cross-env NODE_ENV=production webpack-dev-server --watch --config ./webpack/webpack.config.prod.js",
    "start-dev": "npm run lint && cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "test": "jest",
    "debug": "cross-env NODE_ENV=production webpack --watch --env.debug --config ./webpack/webpack.config.prod.js",
    "debug-dev": "cross-env NODE_ENV=development webpack --watch --env.debug --config ./webpack/webpack.config.dev.js",
    "dev": "npm run start-dev",
    "test-coverage": "jest --coverage",
    "start-storybook": "start-storybook -p 9009 -c .storybook -s public",
    "build-storybook": "cross-env NODE_ENV=development build-storybook --quiet -c .storybook -s public",
    "run-storybook": "serve storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint ./src -c .eslintrc --ext js,jsx,ts,tsx"
